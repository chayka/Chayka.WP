{
    "name": "chayka/wp",
    "description": "Set of classes that allow you to build WP Themes & Plugins in more OOP / MVC style",
    "type": "library",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Boris Mossounov",
            "email": "borix@tut.by"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chayka\\WP\\": "src/Chayka/WP"
        }
    },
    "repositories": [
        {
            "name" : "chayka/helpers",
            "type" : "git",
            "url" : "git@github.com:chayka/Chayka.Helpers.git"
        },
        {
            "name" : "chayka/mvc",
            "type" : "git",
            "url" : "git@github.com:chayka/Chayka.MVC.git"
        }
    ],
    "require":{
        "chayka/mvc": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.4.x-dev"
    }

}